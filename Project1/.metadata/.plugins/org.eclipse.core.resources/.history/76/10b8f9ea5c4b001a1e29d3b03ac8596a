package project1.reimbursement.controller;

import javax.servlet.http.HttpServletRequest;

public class RequestHelper {

	public static String process(HttpServletRequest req){
		System.out.println(req.getRequestURI());
		
		switch(req.getRequestURI()) {
		
		
		
		case "/Showmethemoney/toEmployeeLogIn":
			
			return "resources/html/emp_Login.html";   //good 
			
			
		case "/Showmethemoney/toFinaceLogIn":

			
			return "resources/html/fin_Login.html";
			
			
//		case "/Showmethemoney/toEmployeeLogIn":
//
//			
//			return EMP_LoginController.login(req);
			
			
//			
//			
//			
		default:
			
			return "resources/html/404.html";
		}
		return null;
	}
}